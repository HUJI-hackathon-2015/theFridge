/*@charset "utf-8";*/
/* CSS Document */

#header {
	height: 70px;
	width: 100%;
	background-color: #57607c; 
	
}

#plus {
	height: 42px;
	width: 42px;
	margin-top: 16px;
	margin-right: 20px;
	float: right;
}

#back {
	height: 42px;
	width: 20px;
	margin-top: 16px;
	margin-left: 20px;
	float: left;
	background-image:url(imgs/back.png);
}

#body {
	height: 800px;
	width: 100%;
	background-color: #bfc5e2;
}

#go! {
	height: 100px;
	width: 100px;
	margin-left: 58;
	margin-right: auto;
	margin-bottom: 20px;
	float: right;
		
}

#middle_section {
	height: 616px;
	width: 100%;
	
	background-color: #bfc5e2; 
	
		
}

#bottom_section {
	height: 142px;
	width: 100%;
	border-top-color: #57607c;
	background-color: #bfc5e2;
	
}

.button {
	height: 130px;
	width:130px;
	margin-left: 5%;
	margin-top: 20px;
	background-color:#f45b93;
	border-radius: 25px;
	float: left;
	
}

.row {
	height: 150px;
	width: 100%;
	margin-top: 0px;
}
